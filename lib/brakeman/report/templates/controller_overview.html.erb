<h2>Controllers</h2>

<table>
  <tr>
    <th>Name</th>
    <th>Parent</th>
    <th>Includes</th>
    <th>Routes</th>
  </tr>
<% controller_rows.each do |row| %>
  <tr>
    <td><%= row['Name'] %></td>
    <td><%= row['Parent'] %></td>
    <td><%= row['Includes'] %></td>
    <td><%= row['Routes'] %></td>
  </tr>
<% end %>
</table>